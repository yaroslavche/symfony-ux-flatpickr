{
    "name": "yaroslavche/symfony-ux-flatpickr",
    "type": "symfony-bundle",
    "description": "Flatpickr integration for Symfony",
    "keywords": ["symfony", "ux", "datetime", "daterange"],
    "license": "MIT",
    "authors": [
        {
            "name": "yaroslavche",
            "email": "yaroslav429@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yaroslavche\\Symfony\\UX\\Flatpickr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yaroslavche\\Symfony\\UX\\Flatpickr\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4.5",
        "ext-json": "*",
        "symfony/config": "^5.4|^6.3",
        "symfony/form": "^5.4|^6.3",
        "symfony/dependency-injection": "^5.4|^6.3",
        "symfony/http-kernel": "^5.4|^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^5.4|^6.3",
        "symfony/twig-bundle": "^5.4|^6.3",
        "symfony/var-dumper": "^5.4|^6.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
